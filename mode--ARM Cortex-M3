//#include "systick.h"
//#include "12864.h"
//#include "menu1.h"
//#include "sys.h"
/* Private defines--------------------------------------*/
/* Private function prototypes--------------------------*/
/* Private functions -----------------------------------*/
uint8_t tese_ipbuff[1] = {0xff};
/*新增函数*/
uint32_t Disp_Flag=0;
uint32_t Disp_Time=0;
uint32_t Key_Value=0;
uint32_t Key_Time=0;
uint32_t Disp_State=0;
int main(void)
{ 
	User_BSP_Init();//IO初始化
	Soft_Init();//函数初始化
	JCM12864_Init();
	JCM12864_clear();
	Delay_ms(10);

	
	while(1)
	{        
//		Choose_Bit(0, 0,0,2);//"_"闪烁效果，用于位的选择,part为前八格或后八格(0,1)
//		Input_Port(1, 0, 6,Local_Network_Flag);
		if(System_Flag == 0)//开机状态中
		{	
			if(Run_Tim3_Flag>500)
			{
				Fill_Information();
				PBout(3) = 1;
				}
			else
			{PBout(3) = 0;}
			if( Run_Tim3_Flag>= 2000)//5S后开机运行
			{
				JCM12864_Init(); 
				JCM12864_clear();
				Delay_ms(10);
				BSP_JCM12864_Demo0();		
				System_Flag = 1;
				}
			else
			{
				if(Disp_Time>200)
				{
					BSP_JCM12864_DemoFF();//开机界面
					Disp_Time = 0;
					}
				}
			}
		else
		{
			if(Key_Time>200)
			{
				KEY_Item();
				Key_Time = 0;
				}
			if(Disp_Time>200)
			{
				LCD_Show();
				Disp_Time = 0;
				}
			Fill_Information();

		}
	}
}




